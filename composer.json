{
    "name": "boryszielonka/php-sax-parser",
    "description": "This package makes it simple to use SAX parser for efficient XML parsing.",
    "type": "library",
    "require": {
        "php": "8.4.*",
        "ext-xml": "*",
        "monolog/monolog": "^3.9",
        "ext-libxml": "*",
        "symfony/yaml": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.2",
        "friendsofphp/php-cs-fixer": "^v3.85",
        "phpstan/phpstan": "^2.1.21",
        "squizlabs/php_codesniffer": "^3.7.2"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "BoZielonka\\PhpSaxParser\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BoZielonka\\PhpSaxParser\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Borys Zielonka",
            "email": "boryszielonka@gmail.com"
        }
    ],
    "minimum-stability": "stable"
}
